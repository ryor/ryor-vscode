{
  "$schema": "https://biomejs.dev/schemas/1.9.2/schema.json",
  "formatter": {
    "indentStyle": "space",
    "lineWidth": 200
  },
  "javascript": {
    "formatter": {
      "quoteStyle": "single",
      "semicolons": "asNeeded",
      "trailingCommas": "none"
    }
  },
  "linter": {
    "rules": {
      "performance": {
        "noDelete": "off"
      },
      "style": {
        "useEnumInitializers": "off"
      }
    }
  }
}
